  "description": "The admin UI for the SHOGun Webmapping backend.",
  "main": "index.js",
  "scripts": {
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "pretest": "npm run lint",
    "test": "jest",
    "test:watch": "jest --watch",
